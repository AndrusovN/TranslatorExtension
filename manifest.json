{
	"manifest_version": 2,
	"name": "Translate english to russian",
	"version": "1.0.5",
	"icons": {"128": "icon.png"},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
			  "<all_urls>"
			],
			"js": ["content.js"]
		}
	]
}